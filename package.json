{
  "name": "svelte-app",
  "version": "0.0.2",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "export IS_DEV=1 && npm-run-all --parallel watch:**",
    "watch:extendscript": "webpack --config extendscript.config.js --watch",
    "watch:cep": "sleep 5 && webpack-dev-server --config panel.config.js",
    "build": "npm-run-all --serial build:**",
    "build:extendscript": "webpack --config extendscript.config.js --display verbose",
    "build:cep": "webpack --config panel.config.js --display verbose"
  },
  "cep": {
    "name": "Svelte Starter",
    "id": "cep.svelte.webpack",
    "hosts": "*",
    "extensions": [
      {
        "type": "Panel",
        "name": "___ svelte starter",
        "id": "cep.svelte.webpack.panel",
        "devPort": 8080,
        "htmlFilename": "./panel.html",
        "panelWidth": 840,
        "panelHeight": 600,
        "cefParams": [
          "--allow-file-access-from-files",
          "--allow-file-access",
          "--enable-nodejs",
          "--mixed-context"
        ]
      }
    ]
  },
  "devDependencies": {
    "cep-bundler-webpack": "0.0.57",
    "cep-interface": "^0.5.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "mini-css-extract-plugin": "^0.6.0",
    "npm-run-all": "^4.1.5",
    "style-loader": "^0.23.1",
    "svelte": "^3.0.0",
    "svelte-loader": "2.13.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "@adobe/spectrum-css": "^2.12.0"
  }
}
